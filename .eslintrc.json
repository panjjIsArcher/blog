{
  "extends": "next",
  "rules": {
    "no-unused-vars": [1, { "args": "after-used", "argsIgnorePattern": "^_" }],
    "quotes": ["error", "double"],
    "no-dupe-args": "error",
    "no-var": "error",
    "no-console": "error",
    "react/self-closing-comp": "error"
  },
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "commonjs": true,
    "es6": true
  }
}
